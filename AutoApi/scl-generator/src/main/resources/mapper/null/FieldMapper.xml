<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljm.generator.mapper.FieldMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ljm.generator.pojo.Field">
        <id column="id" property="id" />
        <result column="data_obj_id" property="dataObjId" />
        <result column="field_key" property="fieldKey" />
        <result column="field_name" property="fieldName" />
        <result column="field_type" property="fieldType" />
        <result column="is_delete" property="isDelete" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, data_obj_id, field_key, field_name, field_type, is_delete
    </sql>

</mapper>
